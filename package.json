{
  "name": "customerio-node",
  "description": "A node client for the Customer.io event API. http://customer.io",
  "version": "2.1.0",
  "author": "Customer.io (https://customer.io)",
  "contributors": [
    "Alvin Crespo (https://github.com/alvincrespo)",
    "Jeff Escalante (https://github.com/jescalan)",
    "Artem Ivanov (https://github.com/ivanovart)",
    "Nuno Sousa (https://github.com/nunofgs)"
  ],
  "ava": {
    "verbose": true,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "exclude": [
      "**/*.d.ts",
      "coverage/",
      "test/"
    ],
    "reporter": [
      "text"
    ],
    "cache": false,
    "check-coverage": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100
  },
  "bugs": "https://github.com/customerio/customerio-node/issues",
  "dependencies": {
    "request": "^2.58.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.41",
    "@types/request": "^2.48.5",
    "@types/sinon": "^10.0.0",
    "ava": "^3.15.0",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "sinon": "^10.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "homepage": "https://github.com/customerio/customerio-node",
  "keywords": [
    "customerio",
    "node"
  ],
  "license": "MIT",
  "main": "dist/track.js",
  "typings": "dist/track.d.ts",
  "repository": "customerio/customerio-node",
  "scripts": {
    "test": "nyc ava",
    "build": "tsc -p tsconfig.json",
    "prepublish": "npm run build"
  }
}
